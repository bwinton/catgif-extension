{
  "manifest_version": 2,
  "icons": {
    "48": "icon.png",
    "128": "icon128.png"
  },
  "name": "Cat Gifs!",
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "catgifs@mozilla.org"
    }
  },

  "background": {
